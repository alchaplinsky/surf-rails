<div class="account js-account">
  <div class="avatar">
    {{#if avatar}}
      <img src="{{avatar.url}}" alt="">
    {{else}}
      {{letter}}
    {{/if}}
  </div>
  <div class="last-col">
    <div>{{first_name}} {{last_name}}</div>
    <span class="muted">{{email}}</span>
  </div>
</div>
<div class="list js-list">
  <h2>
    Interests <small class="js-count"></small>
  </h2>
  <div class="add-interest js-add-interest">
    <span class="icon-plus"></span>
    new interest
  </div>
  <div class="js-interests"></div>
</div>
<div class="invite-link js-invite">
  <i class="icon-envelope"></i> Send Invite
</div>
<a class="feedback-link" target="_blank" href="https://surfapp.io/feedback">
  <i class="icon-plane"></i> Feedback
</a>
<div class="sidebar-close-toggle js-sidebar-close-toggle"></div>
